    "express-validator": "^7.3.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.1",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
